{
  "name": "@objectpartners/components",
  "version": "0.0.3",
  "description": "React components to get a kickstart on your next, great OPi project",
  "main": "dist/components.js",
  "module": "dist/components-es.js",
  "devDependencies": {
    "@storybook/react": "~3.4.3",
    "@team-griffin/install-self-peers": "~1.1.1",
    "babel-core": "~6.26.3",
    "babel-plugin-external-helpers": "~6.22.0",
    "babel-plugin-transform-object-rest-spread": "~6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "~0.4.13",
    "babel-preset-env": "~1.6.1",
    "babel-preset-react": "~6.24.1",
    "cross-env": "~5.1.5",
    "del-cli": "~1.1.0",
    "prop-types": "~15.6.1",
    "react-dom": "~16.3.2",
    "rollup": "~0.58.2",
    "rollup-plugin-babel": "~3.0.4",
    "rollup-plugin-node-resolve": "~3.3.0",
    "rollup-plugin-uglify": "~3.0.0"
  },
  "peerDependencies": {
    "emotion": "^9.0.0",
    "react": "^16.0.0",
    "react-emotion": "~9.0.0"
  },
  "scripts": {
    "prepare": "install-self-peers -- --ignore-scripts",
    "prepublishOnly": "npm run build",
    "prebuild": "del-cli \"dist/**/*\"",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9000 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public"
  },
  "publishConfig": {
    "access": "public"
  }
}
